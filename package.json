{
  "name": "gh-pr-review-voodoo",
  "version": "1.1.0",
  "description": "A Probot app",
  "author": "Vitalii Elenhaupt <velenhaupt@gmail.com>",
  "license": "ISC",
  "repository": "https://github.com//gh-pr-review-voodoo.git",
  "homepage": "https://github.com//gh-pr-review-voodoo",
  "bugs": "https://github.com//gh-pr-review-voodoo/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json --outDir public",
    "dev": "nodemon --exec \"npm run build && npm start\"",
    "start": "probot run ./public/index.js",
    "lint": "standard **/*.ts --fix",
    "test": "jest",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "@chadfawcett/probot-serverless-now": "^1.0.0",
    "@types/nock": "^9.3.0",
    "nock": "^10.0.0",
    "probot": "^7.2.0"
  },
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@types/node": "^11.15.20",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^24.0.0",
    "nodemon": "^2.0.4",
    "smee-client": "^1.0.2",
    "standard": "^12.0.1",
    "ts-jest": "^26.1.4",
    "typescript": "^3.9.7",
    "typescript-eslint-parser": "^22.0.0"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "parser": "typescript-eslint-parser",
    "env": [
      "jest"
    ],
    "plugins": [
      "typescript"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
